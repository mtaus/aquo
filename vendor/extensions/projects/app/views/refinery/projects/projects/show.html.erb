<% content_for :body_content_title do %>
  <%= @project.name %>
<% end %>

<% content_for :project do %>
  <section id = "project_detail">
    <%= image_fu @project.logo, nil %>
    <h1><%=raw @project.name %></h1>
    <h2><%=raw @project.tag %></h2>
    <h3><%=raw @project.overview %></h3>
    <%=raw @project.description %>
  </section>
<% end %>

<%= render '/refinery/content_page' %>
