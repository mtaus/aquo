<%= render '/refinery/content_page' %>
<section id="featured_projects">
  <% @projects.each do |project| %>
    <a href="/projects/#{project.id}">
      <section>
        <%=image_fu project.logo, nil %><br />
        <%=raw project.name %><br />
        <%=raw project.overview %>
      </section>
    </a>
  <% end %>
</section>